<?import bfst19.danmarkskort.view.controls.TopMenu?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.Scene?>
<fx:root
    type="javafx.scene.Scene"
    xmlns:fx="http://javafx.com/fxml"
    xmlns="http://javafx.com/javafx"
>
    <BorderPane prefHeight="600.0" prefWidth="1100.0" fx:id="borderPane">
        <top>
            <TopMenu fx:id="topMenu"/>
        </top>
        <center>
            <MapPane fx:id="mapPane"/>
        </center>
        <left>
            <AddressSearchPane fx:id="addressSearchPane" prefWidth="300"/>
        </left>
    </BorderPane>
</fx:root>

